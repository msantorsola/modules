nextflow_process {

    name "Test Process GAMETES_GENERATEDATASETS"
    script "../main.nf"
    process "GAMETES_GENERATEDATASETS"

    tag "modules"
    tag "modules_nfcore"
    tag "gametes"
    tag "gametes/generatedatasets"
    config "./nextflow.config"

    test("dataset - model") {


        when {
            process {
                """
                input[0] = [[ id:'test'],
                file("https://github.com/msantorsola/random_tests/main/model_h0.2_p0.3_a0.3_f0.4_quantile_1_Models.txt")]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out.edmresults).get(0).get(1) ==~ ".*/*._EDM_*" }
                { assert snapshot(process.out.edmresults).get(0).get(1) ==~ ".*/*._EDM_*" }
                )
        }
    }
}

