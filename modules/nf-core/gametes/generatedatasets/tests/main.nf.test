nextflow_process {

    name "Test Process GAMETES_GENERATEDATASETS"
    script "../main.nf"
    process "GAMETES_GENERATEDATASETS"

    tag "modules"
    tag "modules_nfcore"
    tag "gametes"
    tag "gametes/generatedatasets"
    config "./nextflow.config"

    test("dataset - model") {


        when {
            process {
                """
                input[0] = [[ id:'test'],
                file("https://raw.githubusercontent.com/msantorsola/random_tests/main/model_h0.2_p0.3_a0.3_f0.4_Models.txt")]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
                { assert snapshot(process.out.results).match() }            )
        }

    }

}
