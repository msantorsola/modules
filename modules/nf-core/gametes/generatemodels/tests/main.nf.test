nextflow_process {

    name "Test Process GAMETES_GENERATEMODELS"
    script "../main.nf"
    process "GAMETES_GENERATEMODELS"

    tag "modules"
    tag "modules_nfcore"
    tag "gametes"
    tag "gametes/generatemodels"
    config "./nextflow.config"

    test("dataset - model") {


        when {
            process {
                """

                input[0] = [
                    [ id:'test', single_end:false ], // meta map
                    file("https://raw.githubusercontent.com/msantorsola/random_tests/main/h03_p06_a03_a02_a03_f03_p_q_d_t90000_Models.txt"),
                ]
                """
            }
        }


        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}