params {

    // Models options
    prevalence     = 0.2
    attributeAlleleFrequency     = 0.2
    proportion = 0.2
    heritability = 0.3

}



process {

    withName: GAMETES_GENERATEMODELS {
        ext.args = [
            "-h ${params.heritability}", 
            "-p ${params.prevalence}",
            "-a ${params.attributeAlleleFrequency}",
            "-f ${params.proportion}"
        ].join(' ').trim()

    }
}

